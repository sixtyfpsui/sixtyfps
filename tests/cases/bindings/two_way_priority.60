export Compo := Rectangle {
    property<int> foo <=> self.bar;
    property<int> bar: 120;
}

TestCase := Window {
    compo0 := Compo { foo: compo1.foo + 20; }
    compo1 := Compo {}
    compo2 := Compo { foo: compo1.foo + 10; }

    property <int> compo0_foo: compo0.foo;
    property <int> compo2_foo: compo2.foo;
}

/*

```rust
