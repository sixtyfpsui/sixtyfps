TestCase := Rectangle {
    property <bool> toggle: { return false; }
    property <int> value: {
        if (toggle) {
            return 42;
        }
        return 100;
    }
    property <float> value2: {
        return 100;
    }

    callback test_signal;
    property <bool> block_signal;
    property <bool> signal_handled;
    test_signal => {
        if (block_signal) {
            return;
        }
        signal_handled = true;
    }

    property<bool> test: { return value2 == value; return false; }
}
/*
```cpp
