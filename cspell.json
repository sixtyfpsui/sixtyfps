{
    "version": "0.1",
    "language": "en",
    "languageSettings": [
        {
            "languageId": "rust",
            "words": [
                "builtins",
                "concat",
                "Consts",
                "corelib",
                "evenodd",
                "femtovg",
                "Flickable",
                "fullscreen",
                "itertools",
                "MaybeUninit",
                "peekable",
                "Realloc",
                "Rgba",
                "rtti",
                "triomphe",
                "uninit",
                "unmap",
                "viewbox",
                "wasm",
                "winit"
            ],
            "ignoreRegExpList": [
                "/#\\[cfg(.+)\\]/",
                "/#\\[repr(.+)\\]/",
                // Excludes various crate paths
                "/crate::(.+)/",
                "/winit::(.+)/",
                "/core::(.+)/",
                "/std::(.+)/"
            ]
        },
        {
            "languageId": "restructuredtext",
            "words": [
                "genindex",
                "langref",
                "modindex",
                "toctree"
            ],
            "ignoreRegExpList": [
                "/:.+:/"
            ]
        },
        {
            "languageId": "markdown",
            "words": [
                "rustup",
                "SUBDIR",
                "Yocto"
            ],
            "ignoreRegExpList": []
        }
    ],
    "words": [
        "cbindgen",
        "cmake",
        "Goffart",
        "Hausmann",
        "Helvetica",
        "layouting", // This would be worth renaming in the ItemVTable to layout_info
        "listview",
        "MSVC",
        "rustc",
        "SDK",
        "sixtyfps",
        "SPDX",
        "streetsidesoftware",
        "toolchain",
        "vtable"
    ],
    "ignorePaths": [
        "cspell.json",
        "api/sixtyfps-cpp/docs/conf.py"
    ],
    "overrides": [
        {
            "filename": "**/*.rst",
            "languageId": "restructuredtext"
        }
    ]
}